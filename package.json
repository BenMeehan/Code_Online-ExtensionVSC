{
  "name": "runcodeonline",
  "displayName": "Run Code Online",
  "publisher": "BenMeehan111",
  "description": "Run code online directly from VSCode",
  "icon": "images/layers.png",
  "version": "1.1.0",
  "engines": {
    "vscode": "^1.63.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:codeonline.run"
  ],
  "main": "./extension.js",
